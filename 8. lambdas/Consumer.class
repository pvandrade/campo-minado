package lambdas;

import java.util.List;
import java.util.Arrays;
import java.util.function.Consumer;

public class Consumer {
    
    public static void main(String[] args) {
        
        Consumer<Produto> imprimirNome = valor -> System.out.println(p.nome);
        
        Produto p1 = new Produto("Caneta", 12.34, 0.09);
        imprimirNome.accept(p1);
        
        Produto p2 = new Produto("Notebook", 2987.90, 0.25);
        Produto p3 = new Produto("Caderno", 19.90, 0.03);
        Produto p4 = new Produto("Borracha", 1.40, 0.19);
        Produto p5 = new Produto("LÃ¡pis", 2.34, 0.19);
        
        List<Produto> produtos = Arrays.asList(p1, p2, p3, p4, p5);
        
        produtos.forEach(imprimirNome);
        produtos.forEach(p -> System.out.println(p.preco));
        produtos.forEach(System.out::println);
    }
}
